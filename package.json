{
  "name": "@resee-movies/utilities",
  "description": "An eclectic collection of methods used across ReSee Movie projects.",
  "repository": {
    "url": "git+https://github.com/resee-movies/utilities.git"
  },
  "bugs": "https://github.com/resee-movies/utilities/issues",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "files": [
    "./dist"
  ],
  "exports": {
    "./arrays/*": "./dist/arrays/*.js",
    "./dates/*": "./dist/dates/*.js",
    "./dom/*": "./dist/dom/*.js",
    "./geo/*": "./dist/geo/*.js",
    "./images/*": "./dist/images/*.js",
    "./numbers/*": "./dist/numbers/*.js",
    "./objects/*": "./dist/objects/*.js",
    "./resee/*": "./dist/resee/*.js",
    "./seo/*": "./dist/seo/*.js",
    "./sets/*": "./dist/sets/*.js",
    "./strings/*": "./dist/strings/*.js",
    "./timers/*": "./dist/timers/*.js",
    "./tmdb/*": "./dist/tmdb/*.js",
    "./typings/*": "./dist/typings/*.js",
    "./urls/*": "./dist/urls/*.js"
  },
  "scripts": {
    "dev": "rm -rf ./dist && tsc --watch",
    "prepare": "rm -rf ./dist && tsc",
    "release": "dotenv release-it --",
    "docs": "typedoc",
    "lint": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "test:all": "npm run lint && npm test"
  },
  "dependencies": {
    "@drorgl/n-gram": "^1.1.2",
    "@sindresorhus/slugify": "^3.0.0",
    "core-js": "^3.48.0",
    "localized-address-format": "^1.3.3",
    "nanoid": "^5.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@release-it/conventional-changelog": "^10.0.5",
    "@types/node": "^24.3.0",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv-cli": "^11.0.0",
    "eslint": "^10.0.2",
    "playwright": "^1.58.2",
    "release-it": "^19.2.4",
    "typedoc": "^0.28.17",
    "typedoc-github-theme": "^0.4.0",
    "typedoc-plugin-merge-modules": "^7.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  }
}
