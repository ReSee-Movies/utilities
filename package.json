{
  "name": "@resee-movies/utilities",
  "description": "An eclectic collection of methods used across ReSee Movie projects.",
  "repository": {
    "url": "git+https://github.com/resee-movies/utilities.git"
  },
  "bugs": "https://github.com/resee-movies/utilities/issues",
  "version": "0.9.0",
  "private": false,
  "files": [
    "./dist"
  ],
  "exports": {
    "./arrays/*": "./dist/arrays/*.js",
    "./config": "./dist/config.js",
    "./dates/*": "./dist/dates/*.js",
    "./dom/*": "./dist/dom/*.js",
    "./geo/*": "./dist/geo/*.js",
    "./images/*": "./dist/images/*.js",
    "./numbers/*": "./dist/numbers/*.js",
    "./objects/*": "./dist/objects/*.js",
    "./resee/*": "./dist/resee/*.js",
    "./seo/*": "./dist/seo/*.js",
    "./sets/*": "./dist/sets/*.js",
    "./strings/*": "./dist/strings/*.js",
    "./timers/*": "./dist/timers/*.js",
    "./tmdb/*": "./dist/tmdb/*.js",
    "./typings/*": "./dist/typings/*.js",
    "./urls/*": "./dist/urls/*.js"
  },
  "scripts": {
    "dev": "tsc --watch",
    "prepare": "tsc",
    "release": "dotenv release-it --",
    "docs": "typedoc",
    "lint": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "test:all": "npm run lint && npm test"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "core-js": "^3.45.1",
    "localized-address-format": "^1.3.1",
    "nanoid": "^5.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^24.3.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.31.0",
    "playwright": "^1.55.0",
    "release-it": "^19.0.4",
    "typedoc": "^0.28.11",
    "typedoc-github-theme": "^0.3.1",
    "typedoc-plugin-merge-modules": "^7.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4"
  }
}
